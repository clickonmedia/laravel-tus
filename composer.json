{
    "name": "clickon-iq/laravel-tus-php",
    "description": "This is my package laravel-tus",
    "keywords": [
        "clickonmedia",
        "laravel",
        "laravel-tus"
    ],
    "homepage": "https://github.com/clickonmedia/laravel-tus",
    "license": "MIT",
    "authors": [
        {
            "name": "Levi Strange",
            "email": "levi.strange@clickon.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "ankitpokhrel/tus-php": "^2.1",
        "illuminate/contracts": "^8.37",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Clickonmedia\\LaravelTus\\": "src",
            "Clickonmedia\\LaravelTus\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Clickonmedia\\LaravelTus\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Clickonmedia\\LaravelTus\\LaravelTusServiceProvider"
            ],
            "aliases": {
                "LaravelTus": "Clickonmedia\\LaravelTus\\LaravelTusFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
